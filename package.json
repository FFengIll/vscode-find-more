{
	"name": "find-more",
	"displayName": "find-more",
	"description": "Extend of the build-in find functions",
	"icon": "images/markdownlint-128.png",
	"author": "Feng Youzheng",
	"version": "0.0.1",
	"publisher": "FengYouzheng",
	"license": "MIT",
	"homepage": "https://github.com/DavidAnson/vscode-markdownlint",
	"repository": {
		"type": "git",
		"url": "https://github.com/DavidAnson/vscode-markdownlint.git"
	},
	"bugs": "https://github.com/DavidAnson/vscode-markdownlint/issues",

	"engines": {
		"vscode": "^1.10.0"
	},
	"categories": [
		"Other"
	],
	"dependencies": {
		"markdownlint": "^0.4.1"
	},
	"keywords": [
		"find",
		"lint"
	],
	"activationEvents": [
		"onLanguage:markdown",
		"onCommand:find.findAll",
		"onCommand:find.jump"
	],
	"main": "./out/src/extension",
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"contributes": {
		"menus": {
			"editor/title": [{
				"when": "findWidgetVisible",
				"command": "find.findAll",
				"group": "navigation"
			}],
			"editor/context": [{
				"command": "find.jump",
				"group": "navigation"
			}]
		},
		"commands": [{
				"command": "find.findAll",
				"title": "Find all"
			},
			{
				"command": "find.jump",
				"title": "Jump to"
			}
		],
		"jsonValidation": [{
			"fileMatch": ".markdownlint.json",
			"url": "./node_modules/markdownlint/schema/markdownlint-config-schema.json"
		}],
		"configuration": {
			"title": "markdownlint configuration",
			"type": "object",
			"properties": {
				
			}
		}
	}
}